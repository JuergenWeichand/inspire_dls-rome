<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <groupId>de.weichand.inspire</groupId>
    <artifactId>inspire_dls-rome</artifactId>
    <version>1.5.0</version>
    <packaging>jar</packaging>

    <name>INSPIRE_DLS ROME Module</name>
    <url>http://www.weichand.de/</url>

    <issueManagement>
        <url>https://github.com/JuergenWeichand/inspire_dls-rome/issues</url>
    </issueManagement>

    <scm>
        <connection>scm:git:git@github.com:JuergenWeichand/inspire_dls-rome.git</connection>
        <developerConnection>scm:git:git@github.com:JuergenWeichand/inspire_dls-rome.git</developerConnection>
        <url>https://github.com/JuergenWeichand/inspire_dls-rome</url>
    </scm>

    <developers>
        <developer>
            <email>juergen@weichand.de</email>
            <name>JÃ¼rgen Weichand</name>
        </developer>
        <developer>
            <email>tobias.clement@it.nrw.de</email>
            <name>Tobias Clement</name>
        </developer>
    </developers>

    <licenses>
        <license>
            <name>The Apache Software License, Version 2.0</name>
            <url>http://www.apache.org/licenses/LICENSE-2.0.txt</url>
            <distribution>repo</distribution>
        </license>
    </licenses>

    <properties>
        <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
    </properties>

    <dependencies>
        <dependency>
            <groupId>com.rometools</groupId>
            <artifactId>rome</artifactId>
            <version>1.5.0</version>
        </dependency> 
        <dependency>
            <groupId>com.rometools</groupId>
            <artifactId>rome-modules</artifactId>
            <version>1.5.0</version>
            <scope>test</scope>
        </dependency>                 
        <dependency>
            <groupId>junit</groupId>
            <artifactId>junit</artifactId>
            <version>3.8.1</version>
            <scope>test</scope>
        </dependency>
    </dependencies>
    
    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>3.3</version>
                <configuration>
                    <source>1.7</source>
                    <target>1.7</target>
                </configuration>
            </plugin>
        </plugins>
    </build>
    
</project>
